<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<connectionStrings>
		<add connectionString="Data Source=localhost;Initial Catalog=hublisher;Integrated Security=True;" name="hublisherEntities" providerName="System.Data.SqlClient"/>
	</connectionStrings>
	<system.web>
		<compilation debug="true" targetFramework="4.0"/>
	</system.web>
	<system.serviceModel>
		<bindings>
   <basicHttpBinding>
    <binding name="WorkflowEngineSOASoap" />
    <binding name="EntityManagerSOASoap" />
   </basicHttpBinding>
   <customBinding>
    <binding name="WorkflowEngineSOASoap12">
     <textMessageEncoding messageVersion="Soap12" />
     <httpTransport />
    </binding>
    <binding name="EntityManagerSOASoap12">
     <textMessageEncoding messageVersion="Soap12" />
     <httpTransport />
    </binding>
   </customBinding>
    <wsHttpBinding>
        <binding openTimeout="00:10:00" 
                 closeTimeout="00:10:00" 
                 sendTimeout="00:10:00" 
                 receiveTimeout="00:10:00">
        </binding>
      </wsHttpBinding>
  </bindings>
		<client>
   <endpoint address="http://172.17.40.93/Procesos/webservices/workflowenginesoa.asmx"
    binding="basicHttpBinding" bindingConfiguration="WorkflowEngineSOASoap"
    contract="BizagiCapaSOA.WorkflowEngineSOASoap" name="WorkflowEngineSOASoap" />
   <endpoint address="http://172.17.40.93/Procesos/webservices/entitymanagersoa.asmx"
    binding="basicHttpBinding" bindingConfiguration="EntityManagerSOASoap"
    contract="BizagiSOAEntity.EntityManagerSOASoap" name="EntityManagerSOASoap" />
  </client>
		<services>
			<!--AGREGAR NOMBRE DE SERVICIO Y TIPO CONFIGURACIÓN-->
			<service behaviorConfiguration="ServiceBehaviour" name="wsEmaresaWCF.EmaresaService">
				<endpoint address="" behaviorConfiguration="web" binding="webHttpBinding" contract="wsEmaresaWCF.IEmaresaService"/>
			</service>
		</services>
		<behaviors>
			<!--CONFIGURACIÓN DE BEHAVIOR-->
			<serviceBehaviors>
				<behavior name="ServiceBehaviour">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="false"/>
				</behavior>
			</serviceBehaviors>
			<endpointBehaviors>
				<behavior name="web">
					<webHttp helpEnabled="true"/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<serviceHostingEnvironment multipleSiteBindingsEnabled="true"/>
	</system.serviceModel>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true"/>
		<!--
        Para examinar el directorio raíz de la aplicación web durante la depuración, establezca el valor siguiente en true.
        Establézcalo en false antes de la implementación para evitar revelar información sobre la carpeta de aplicación web.
      -->
		<directoryBrowse enabled="true"/>
	</system.webServer>
	<appSettings>
		<add key="domain" value="domain"/>
		<add key="admon" value="admon"/>
	</appSettings>
</configuration>